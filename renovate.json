{
  "extends": [
    "config:base",
    ":gitSignOff",
    ":disableDependencyDashboard"
  ],
  "postUpgradeTasks": {
    "commands": [
      "make gen-golden -e instance=defaults",
      "make gen-golden -e instance=openshift"
    ],
    "fileFilters": [ "tests/golden/**" ],
    "executionMode": "update"
  },
  "suppressNotifications": [ "artifactErrors" ],
  "labels": [
    "dependency"
  ]
}
